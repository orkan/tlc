{
	"name": "orkan/tlc",
	"description": "Simple PHP/cURL (T)ransport framework with (L)ogger, (C)ache and more!",
	"homepage": "https://github.com/orkan/tlc",
	"keywords": [
		"net",
		"curl",
		"flaresolverr",
		"scrap",
		"crawler"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Orkan",
			"role": "Lead",
			"email": "orkans+tlc@gmail.com"
		}
	],
	"require": {
		"php": "^7.4",
		"orkan/utils": "^11.2"
	},
	"require-dev": {
		"monolog/monolog": "^2.9",
		"phpunit/phpunit": "^9.6"
	},
	"autoload": {
		"psr-4": {
			"Orkan\\TLC\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Orkan\\TLC\\Tests\\": "tests"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.0.x-dev",
			"dev-develop": "3.x-dev"
		}
	}
